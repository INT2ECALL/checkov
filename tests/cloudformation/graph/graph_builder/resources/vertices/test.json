{
  "AWSTemplateFormatVersion" : "2010-09-09",
  "Description": "AWS CloudFormation Template to deploy insecure infrastructure",
  "Parameters": {
    "KmsMasterKeyId": {
      "Description": "Company Name",
      "Type": "String",
      "Default": "kms_id"
    },
    "DBName": {
      "Description": "Name of the Database",
      "Type": "String",
      "Default": "db"
    }
  },
  "Resources": {
    "MySourceQueue": {
      "Type": "AWS::SQS::Queue",
      "Properties": {
        "Tags": [
          {
            "Key": "yor_trace",
            "Value": "350a913a-86ec-4b4f-a08f-8ddb1e435039"
          }
        ],
        "KmsMasterKeyId": { "Ref": "KmsMasterKeyId" }
      }
    },
    "MyDB": {
      "Type": "AWS::RDS::DBInstance",
      "Properties": {
        "Tags": [
          {
            "Key": "yor_trace",
            "Value": "cd768b08-303e-441d-9a6c-32f949b51746"
          }
        ],
        "DBName": { "Ref": "DBName" },
        "DBInstanceClass": "db.t3.micro",
        "Engine": "mysql",
        "MasterUsername": "master",
        "MasterUserPassword": "password"
      }
    }
  },
  "Outputs": {
    "DBAppPublicDNS": {
      "Description": "DB App Public DNS Name",
      "Value": { "Fn::GetAtt" : [ "MyDB", "PublicDnsName" ] }
    }
  }
}