{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
        "DbSubnetIpBlocks": {
            "Description": "Comma-delimited list of three CIDR blocks",
            "Type": "CommaDelimitedList",
            "Default": "10.0.48.0/24, 10.0.112.0/24, 10.0.176.0/24"
        }
    },
    "Resources": {
        "VPC": {
            "Type": "AWS::EC2::VPC",
            "Properties": {
                "Tags": [
                  {
                    "Key": "yor_trace",
                    "Value": "bce9bc0a-f2a6-46a8-a733-1ef7e1f8b944"
                  }
                ],
                "CidrBlock": "172.16.0.0/16",
                "EnableDnsSupport": true,
                "EnableDnsHostnames": true
            }
        },
        "Subnet0": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "Tags": [
                  {
                    "Key": "yor_trace",
                    "Value": "e6ebcb02-2f38-4a6f-ac68-6bc5e725914e"
                  }
                ],
                "VpcId": {
                    "Ref": "VPC"
                },
                "CidrBlock": {
                    "Fn::Select": [
                        0,
                        {
                            "Ref": "DbSubnetIpBlocks"
                        }
                    ]
                }
            }
        }
    },
    "Outputs": {
        "GrapesSelect": {
            "Description": "Grapes Select",
            "Value": {
                "Fn::Select": [
                    "1",
                    [
                        "apples",
                        "grapes",
                        "oranges",
                        "mangoes"
                    ]
                ]
            }
        },
        "OutOfBoundSelect": {
            "Description": "Out of Bound Select",
            "Value": {
                "Fn::Select": [
                    "7",
                    [
                        "apples",
                        "grapes",
                        "oranges",
                        "mangoes"
                    ]
                ]
            }
        }
    }
}