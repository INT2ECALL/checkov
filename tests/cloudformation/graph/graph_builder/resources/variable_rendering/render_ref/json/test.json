{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "AWS CloudFormation Template to deploy insecure infrastructure",
    "Parameters": {
        "KmsMasterKeyId": {
            "Description": "Company Name",
            "Type": "String"
        },
        "DBName": {
            "Description": "Name of the Database",
            "Type": "String",
            "Default": "db1"
        }
    },
    "Resources": {
        "MySourceQueue": {
            "Type": "AWS::SQS::Queue",
            "Properties": {
                "Tags": [
                  {
                    "Key": "yor_trace",
                    "Value": "4e29eafb-54ef-4281-9ebb-20e5ed3b9559"
                  }
                ],
                "KmsMasterKeyId": {
                    "Ref": "KmsMasterKeyId"
                }
            }
        },
        "MyDB": {
            "Type": "AWS::RDS::DBInstance",
            "Properties": {
                "Tags": [
                  {
                    "Key": "yor_trace",
                    "Value": "926165b9-7079-4682-8181-da41beb87aa0"
                  }
                ],
                "DBName": {
                    "Ref": "DBName"
                },
                "DBInstanceClass": "db.t3.micro",
                "Engine": "mysql",
                "MasterUsername": "master",
                "MasterUserPassword": "password"
            }
        }
    },
    "Outputs": {
        "MyDBInstanceName": {
            "Description": "My DB Instance Name",
            "Value": {
                "Ref": "MyDB"
            }
        }
    }
}